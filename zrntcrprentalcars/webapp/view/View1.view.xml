<mvc:View controllerName="zrntcrprentalcars.controller.View1"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
        <GenericTile
				id="VehiclesTitle"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
				header="Administração Veículos"
				press="pressBrands"
			>
				<TileContent
					id="VehiclesContent"
					footer="Gerenciar Veículos"
				>
					<ImageContent
						id="VehiclesImage"
						src="sap-icon://car-rental"
					/>
				</TileContent>
			</GenericTile>
        </content> 
    </Page>
</mvc:View>
